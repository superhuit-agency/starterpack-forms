{
    "name": "superhuit/starterpack-forms",
    "description": "Starterpack's Form Custom Post Type.",
    "keywords": [
        "wordpress",
        "custom-post-type",
        "form"
    ],
    "license": "GPL-3.0-or-later",
    "homepage": "https://github.com/superhuit-agency/starterpack-forms",
    "authors": [
        {
            "name": "superhuit",
            "email": "tech@superhuit.ch",
            "homepage": "https://superhuit.ch",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/superhuit-agency/starterpack-forms/issues"
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/installers": false,
            "johnpbloch/wordpress-core-installer": false
        }
    },
    "require": {
        "php": ">=7.4",
        "composer/installers": "^2.2",
        "symfony/mime": "^5.3",
        "symfony/validator": "^5.3"
    },
    "require-dev": {
        "wpackagist-plugin/wp-graphql": "^1.27"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ]
}
